form.form(name='form', ng-submit='submit()', novalidate)
	.modal-header
		button.close(type='button', data-dismiss='modal', ng-click='$dismiss()') &times;
		h3.modal-title Filtruj lekarzy

	.modal-body
		.checkbox(ng-repeat='doctor in doctors')
			label
				<input type="checkbox" ng-model='doctor.isSelected'>{{doctor.getDisplayName()}}</input>

	.modal-footer
		button.btn.btn-primary(type='submit') Zapisz